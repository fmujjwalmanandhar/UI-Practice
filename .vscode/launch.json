{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Expo Dev Server",
      "program": "${workspaceFolder}/node_modules/expo/bin/expo.js",
      "args": ["start", "--dev-client"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "iOS",
      "program": "${workspaceFolder}/node_modules/expo/bin/expo.js",
      "args": ["run:ios"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Android",
      "program": "${workspaceFolder}/node_modules/expo/bin/expo.js",
      "args": ["run:android"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Radon IDE panel",
      "type": "radon-ide",
      "request": "launch",
      "appRoot": "./",
      "env": {},
      "ios": {},
      "android": {},
      "usePrebuild": false
    }
  ]
}
